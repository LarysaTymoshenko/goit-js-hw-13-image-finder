fore {\r\n    content: '';\r\n    width: 100%;\r\n    height: 100%;\r\n    border-width: 10px 10px 0 0;\r\n    border-style: solid;\r\n    border-color: rgb(243, 180, 180);\r\n    transition: 200ms $cubic;\r\n    display: block;\r\n    transform-origin: 100% 0;\r\n  }\r\n\r\n  &:after {\r\n    content: '';\r\n    float: left;\r\n    position: relative;\r\n    top: -100%;\r\n    width: 100%;\r\n    height: 100%;\r\n    border-width: 0 10px 0 0;\r\n    border-style: solid;\r\n    border-color: white;\r\n    transform-origin: 100% 0;\r\n    transition: 200ms $cubic;\r\n  }\r\n\r\n  &:hover::after {\r\n    transform: rotate(45deg);\r\n    height: 120%;\r\n    top: -88%;\r\n  }\r\n  &:hover::before {\r\n    transform: scale(0.8);\r\n  }\r\n}\r\n.scroll-to-top.active {\r\n  display: block;\r\n}\r\n.is-hidden {\r\n  display: none;\r\n}\r\n.is-shown {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.material-icons {\r\n  font-family: 'Material Icons';\r\n  font-weight: normal;\r\n  font-style: normal;\r\n  font-size: 25px; /* Preferred icon size */\r\n  display: inline-block;\r\n  line-height: 1;\r\n  text-transform: none;\r\n  letter-spacing: normal;\r\n  word-wrap: normal;\r\n  white-space: nowrap;\r\n  direction: ltr;\r\n  margin: 10px;\r\n\r\n  /* Support for all WebKit browsers. */\r\n  -webkit-font-smoothing: antialiased;\r\n  /* Support for Safari and Chrome. */\r\n  text-rendering: optimizeLegibility;\r\n\r\n  /* Support for Firefox. */\r\n  -moz-osx-font-smoothing: grayscale;\r\n\r\n  /* Support for IE. */\r\n  font-feature-settings: 'liga';\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"8191515f49c25ad2ee497581fd4cdd76","cacheData":{"env":{}}}