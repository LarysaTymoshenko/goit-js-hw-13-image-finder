argin-left: -20px;\r\n  padding: 0;\r\n}\r\n\r\nli {\r\n  margin-top: 20px;\r\n  margin-left: 20px;\r\n  list-style: none;\r\n}\r\n\r\n.stats {\r\n  display: flex;\r\n  text-align: center;\r\n  padding: auto;\r\n  justify-content: space-evenly;\r\n}\r\n\r\n.stats-item {\r\n  display: flex;\r\n  text-align: center;\r\n  width: 40px;\r\n}\r\n\r\n.photo-item {\r\n  border-radius: 4px;\r\n  margin-bottom: 15px;\r\n  max-width: 650px;\r\n\r\n  @media screen and (min-width: 768px) {\r\n    height: 485px;\r\n  }\r\n\r\n  @media screen and (min-width: 1300px) {\r\n    height: 485px;\r\n  }\r\n}\r\n\r\n.gallery {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n}\r\n\r\n.button {\r\n  font-family: $font-family;\r\n  font-style: normal;\r\n  font-size: 20px;\r\n  padding-left: 0px;\r\n  background-color: rgb(157, 232, 247);\r\n  border-radius: 20px;\r\n  cursor: pointer;\r\n  transition: filter 0.3s $cubic;\r\n  width: 100px;\r\n  text-align: center;\r\n  &:hover {\r\n    filter: brightness(0.8);\r\n  }\r\n}\r\n.input-form {\r\n  width: 400px;\r\n  background: rgb(211, 240, 220);\r\n  border-radius: 10px;\r\n  margin: 40px 0;\r\n  &:focus::placeholder {\r\n    color: lightcyan;\r\n  }\r\n}\r\n.scroll-to-top {\r\n  display: none;\r\n  margin: 0;\r\n  padding: 0;\r\n  border: none;\r\n  width: 60px;\r\n  height: 60px;\r\n  box-sizing: border-box;\r\n  position: fixed;\r\n  right: 40px;\r\n  bottom: 40px;\r\n  cursor: pointer;\r\n  background-color: transparent;\r\n  transform: rotate(-45deg);\r\n\r\n  &::before {\r\n    content: '';\r\n    width: 100%;\r\n    height: 100%;\r\n    border-width: 10px 10px 0 0;\r\n    border-style: solid;\r\n    border-color: rgb(243, 180, 180);\r\n    transition: 200ms $cubic;\r\n    display: block;\r\n    transform-origin: 100% 0;\r\n  }\r\n\r\n  &:after {\r\n    content: '';\r\n    float: left;\r\n    position: relative;\r\n    top: -100%;\r\n    width: 100%;\r\n    height: 100%;\r\n    border-width: 0 10px 0 0;\r\n    border-style: solid;\r\n    border-color: white;\r\n    transform-origin: 100% 0;\r\n    transition: 200ms $cubic;\r\n  }\r\n\r\n  &:hover::after {\r\n    transform: rotate(45deg);\r\n    height: 120%;\r\n    top: -88%;\r\n  }\r\n  &:hover::before {\r\n    transform: scale(0.8);\r\n  }\r\n}\r\n.scroll-to-top.active {\r\n  display: block;\r\n}\r\n.is-hidden {\r\n  display: none;\r\n}\r\n.is-shown {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.material-icons {\r\n  font-family: 'Material Icons';\r\n  font-weight: normal;\r\n  font-style: normal;\r\n  font-size: 25px; /* Preferred icon size */\r\n  display: inline-block;\r\n  line-height: 1;\r\n  text-transform: none;\r\n  letter-spacing: normal;\r\n  word-wrap: normal;\r\n  white-space: nowrap;\r\n  direction: ltr;\r\n  margin: 10px;\r\n\r\n  /* Support for all WebKit browsers. */\r\n  -webkit-font-smoothing: antialiased;\r\n  /* Support for Safari and Chrome. */\r\n  text-rendering: optimizeLegibility;\r\n\r\n  /* Support for Firefox. */\r\n  -moz-osx-font-smoothing: grayscale;\r\n\r\n  /* Support for IE. */\r\n  font-feature-settings: 'liga';\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"d1a7d77d5eb9fb9b65887cff20d37462","cacheData":{"env":{}}}